{
  "name": "mikaleb-blog",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "tailwind": "npx tailwindcss -i ./src/assets/main.scss -o ./dist/output.css --watch",
    "serve": "vite serve"
  },
  "dependencies": {
    "@jamescoyle/vue-icon": "^0.1.2",
    "@mdi/js": "^7.4.47",
    "@unhead/vue": "^1.11.7",
    "@vueuse/core": "^10.11.1",
    "@vueuse/motion": "^2.2.6",
    "@vueuse/sound": "^2.0.1",
    "clsx": "^2.1.1",
    "pinia": "^2.2.4",
    "tailwind-merge": "^2.2.2",
    "vue": "^3.5.11",
    "vue-i18n": "^9.14.1",
    "vue-router": "^4.4.5",
    "vue-turnstile": "^1.0.11",
    "vue3-konami-code": "^1.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^18.19.55",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.29.0",
    "jsdom": "^24.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "prettier": "^3.2.5",
    "sass": "^1.79.5",
    "start-server-and-test": "^2.0.8",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^2.1.2",
    "vue-tsc": "^2.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
